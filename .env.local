# PowerBI Configuration
# Azure AD Application Registration
POWERBI_CLIENT_ID=your-azure-app-client-id
POWERBI_CLIENT_SECRET=your-azure-app-client-secret
POWERBI_TENANT_ID=your-azure-tenant-id

# PowerBI Scope (usually this default value)
POWERBI_SCOPE=https://analysis.windows.net/powerbi/api/.default

# PowerBI Report Configuration
POWERBI_REPORT_ID=your-powerbi-report-id
POWERBI_WORKSPACE_ID=your-powerbi-workspace-id
POWERBI_DATASET_ID=your-powerbi-dataset-id
# Embed URL (base URL for programmatic embedding - DO NOT use public sharing URL with autoAuth)
POWERBI_EMBED_URL=https://app.powerbi.com/reportEmbed

# Alternative environment variable names (for compatibility)
POWER_BI_CLIENT_ID=your-azure-app-client-id
POWER_BI_CLIENT_SECRET=your-azure-app-client-secret
POWER_BI_TENANT_ID=your-azure-tenant-id

# Clerk Authentication Configuration
# Get these from your Clerk Dashboard: https://dashboard.clerk.com/
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your-publishable-key-here
CLERK_SECRET_KEY=sk_test_your-secret-key-here

# Clerk URLs (customize these based on your routing preferences)
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/